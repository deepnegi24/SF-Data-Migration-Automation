	<bean id="insert_Account" class="com.salesforce.dataloader.process.ProcessRunner">
		<description>Insert Account</description>
		<property name="name" value="insert_Account"/>
		<property name="configOverrideMap">
			  <map>
			  
				<!-- Salesforce login -->
				<entry key="sfdc.username" value="kuldeep.negi@company.co.nz.test.preprod"/>
				<entry key="sfdc.password" value="e81e56faf49d0365482aa75ccf1a61bd2f2d546cbb368c2143c04e46dd43a4b6ec6342b96d51e7affc"/>
				<entry key="sfdc.endpoint" value="https://test.salesforce.com"/>
				<entry key="sfdc.oauth.enabled" value="false"/>
				<entry key="sfdc.oauth.loginfrombrowser" value="false"/>
				<entry key="sfdc.oauth.clientid" value=""/>
				<entry key="sfdc.debugMessages" value="true"/>
				<!-- Object & operation -->
				
				<entry key="sfdc.entity" value="Account"/>
				<entry key="process.operation" value="upsert"/>
				<entry key="sfdc.externalIdField" value="RecordId__c"/>
				
				<!-- Input CSV -->
				<entry key="dataAccess.type" value="csvRead"/>
				<entry key="dataAccess.readUTF8" value="true"/>
				<entry key="dataAccess.validateCSV" value="false"/>
				<entry key="process.useEuropeanDates" value="true"/>
				<entry key="dataAccess.readDateFormat" value="dd/MM/yyyy"/>
				<entry key="dataAccess.name" value="C:\\Users\\kuldeep.negi\\dataloader_win_v56.0.6\\data\\Account.csv"/>
				<entry key="sfdc.batchSize" value="500"/>
				<entry key="process.enableLastRunOutput" value="true"/>
				<!-- Field mapping -->
				<entry key="process.mappingFile" value="C:\\Users\\kuldeep.negi\\dataloader_win_v56.0.6\\mapping\\Account_Mapping.sdl"/>
				
				<!-- Log files-->
				<entry key="process.outputSuccess" value="C:\\NR Report\\Reports\\Account_success.csv"/>
				<entry key="process.outputError" value="C:\\NR Report\\Reports\\Account_error.csv"/>
	
			  </map>
		</property>
	</bean>
